<!-- Sections -->
<section class="section-1">
  <div class="container d-flex justify-content-center custom-border-3 pb-4">
    <div class="row text-center mt-5">
      <h4 class="pb-0 custom-border-2 custom-font-1"><p class="mb-1 ml-3">Get admitted in reputed institutes across the country.</p></h4>
    </div>
  </div>
  <div class="container d-flex justify-content-center mt-5">
    <div class="card card-outline-secondary w-50">
            <div class="card-header d-flex justify-content-center">
                 <h3 class="mb-0">Register for free</h3>
            </div>
          <!--Card body-->
            <form method="POST" action="/admissions">
                <div class="card-body cus-background1">
                     <div class="form-group row">
                        <label class="col-sm-4 col-form-control form-control-label">Name:</label>
                        <div class="col-sm-8">
                            <input class="form-control" type="text" name="name" required>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-4 col-form-control form-control-label" >Course preferred:</label>
                        <div class="col-sm-8">
                            <input class="form-control" type="text" name="course" required>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-4 col-form-control form-control-label" >Preferred Institute:</label>
                        <div class="col-sm-8">
                            <input class="form-control" type="text" name="Institute" required>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-4 col-form-control form-control-label " >Preferred City:</label>
                        <div class="col-sm-8">
                            <input class="form-control" type="text" name="city" required>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-4 col-form-control form-control-label">Phone No:</label>
                        <div class="col-sm-8">
                            <input class="form-control" type="number" name="phone" required>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-4 col-form-control form-control-label">Email:</label>
                        <div class="col-sm-8">
                            <input class="form-control" type="email" name="email" required>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-4 col-form-control form-control-label"></label>
                        <div class="col-sm-4">
                            <input class="form-control btn btn-primary btn-lgy custom" type="submit">
                        </div>

                    </div>
                </div>
            </form>
           <!--Card body-->
        </div>
  </div>
</section>               

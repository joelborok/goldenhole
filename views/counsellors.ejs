<!-- Sections -->
<section class="section-1 pt-3">
  <div class="container">
                    <div class="row d-flex justify-content-center">
                      <div class="heading-title text-center">
                            <h3 class="text-uppercase custom-font-1">Our Counselors </h3>
                            <p class="p-top-30 half-txt">Nam pulvinar vitae neque et porttitor. Praesent sed nisi eleifend. Nam pulvinar vitae neque et porttitor. Praesent sed nisi eleifend. </p>
                        </div>
                    </div>
                    <div class="row">
                      <%counsellors.forEach(function(counsellor){%>
                        <div class="col-md-4 col-sm-4">
                            <div class="team-member">
                                <div class="team-img ">
                                    <img src="<%=counsellor.image%>" alt="team member" class="img-fluid custom">
                                </div>
                                <a href="/counsellors/<%=counsellor._id%>">
                                <div class="team-hover">
                                    <div class="desk">
                                        <p><%=counsellor.bio_data.substring(0,60)%></p>
                                    </div>
                                </div>
                                </a>
                            </div>
                            <div class="team-title">
                                <a href="/counsellors/<%=counsellor._id%>"><h5><%=counsellor.name%></h5></a>
                                <span><%=counsellor.designation%></span>
                            </div>
                        </div>
                      <%});%>
                </div>
</section>               
<!-- Sections -->
<!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
